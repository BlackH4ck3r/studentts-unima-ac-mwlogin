{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/bootstrap/(.*)",
      "dest": "/public/bootstrap/$1"
    },
    {
      "src": "/dist/(.*)",
      "dest": "/public/dist/$1"
    },
    {
      "src": "/font-awesome/(.*)",
      "dest": "/public/font-awesome/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/login.php.html"
    }
  ]
}
